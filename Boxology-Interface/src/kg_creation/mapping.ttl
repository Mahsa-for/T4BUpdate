		rr:objectMap [
			rml:reference "label"
		]
	];
	rr:predicateObjectMap [
		rr:predicate t4b:hasInput;
		rr:objectMap [
			rr:template "http://tool4boxology.org/Component/{input[*].id}";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate t4b:hasOutput;
		rr:objectMap [
			rr:template "http://tool4boxology.org/Component/{output[*].id}";
		]
	];		
	rr:predicateObjectMap [
		rr:predicate t4b:hasProcess;
		rr:objectMap [ 
			rr:template "http://tool4boxology.org/Component/{process.id}";
		]
	].


